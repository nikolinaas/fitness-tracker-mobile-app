        ["android.permission.ACTIVITY_RECOGNITION",
        "android.permission.RECORD_AUDIO"
      ],
      
      "package": "com.nikolinaas.mobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b891cc8e-1464-4945-a7fb-e9c3105a3f29"
      }
    }
  }
}
